<li>
    <label>Country:</label>
    <input type="text" name="country" value="${adress.country}" placeholder="Belarus" pattern="^[а-яА-ЯёЁa-zA-Z ]+$"/>
</li>
<li>
    <label>City:</label>
    <input type="text" name="city" value="${adress.city}" placeholder="Minsk" pattern="^[а-яА-ЯёЁa-zA-Z ]+$"/>
</li>
<li>
    <label>Street:</label>
    <input type="text" name="street" value="${adress.street}" placeholder="Surganova" pattern="^[а-яА-ЯёЁa-zA-Z0-9 ]+$"/>
</li>
<li>
    <label>Home:</label>
    <input type="number" name="home" value="${adress.home}" placeholder="34"/>
</li>
<li>
    <label>Apartment:</label>
    <input type="number" name="apartment" value="${adress.apartment}" placeholder="12"/>
</li>
<li>
    <label>Postcode:</label>
    <input type="number" name="postcode" value="${adress.postcode}" placeholder="223451"/>
</li>